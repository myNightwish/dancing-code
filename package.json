{
  "name": "mi-element-ui",
  "version": "1.0.3",
  "description": "A Vue.js 2.X UI Toolkit for Web, learning from elementUI from 0 to 1",
  "main": "./lib/mi-ui.common.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cross-env NODE_ENV=development webpack --config build/webpack.config.js",
    "build:file": "node build/bin/iconInit.js & node build/bin/build-entry.js",
    "build:theme": "node build/bin/gen-cssfile && gulp build --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/lib lib/theme-chalk",
    "dev": "npm run build:file & cross-env NODE_ENV=development webpack serve --config build/webpack.config.js",
    "dist": "cross-env NODE_ENV=production webpack --config build/webpack.common.js ",
    "lint": "eslint examples",
    "lint:fix": "eslint --fix examples",
    "format": "cross-env NODE_ENV=development prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
