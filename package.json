    "@babel/preset-react": "^7.24.7",
    "babel-loader": "^9.1.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "clean-webpack-plugin": "^4.0.0",
